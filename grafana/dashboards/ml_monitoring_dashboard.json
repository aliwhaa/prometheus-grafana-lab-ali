    {
      "title": "Current Model Accuracy",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "targets": [{ "expr": "model_accuracy", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "min": 0, "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.75 },
              { "color": "green", "value": 0.8 }
            ]
          }
        }
      }
    },
    {
      "title": "Predictions Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 0 },
      "targets": [{ "expr": "rate(inference_requests_total[1m])", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "req/s" } }
    },
    {
      "title": "Data Lake Response Delay",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [{ "expr": "response_delay_seconds", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Distribution Drift Detected",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [{ "expr": "distribution_drift_detected", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Normal", "color": "green" }, "1": { "text": "DRIFT!", "color": "red" } } }
          ]
        }
      }
    },
    {
      "title": "Total Retrain Count",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [{ "expr": "retrain_count_total", "refId": "A" }],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "blue" } } }
    }
  ],
